{
  "name": "eyes-testcafe",
  "version": "2.1.0",
  "description": "Applitools Eyes SDK for TestCafe",
  "main": "index.js",
  "scripts": {
    "lint": "eslint '**/*.js'",
    "render": "node ../sdk-shared/src/render.js",
    "test": "yarn test:it",
    "test:debug": "yarn test:it:debug",
    "test:it": "testcafe chrome:headless ./test/it/*.spec.js",
    "test:it:debug": "testcafe --inspect-brk chrome:headless ./test/it/*.spec.js",
    "coverage:generate": "coverage-tests create-tests",
    "coverage:sandbox": "coverage-tests create-tests && yarn test:coverage && coverage-tests process-report",
    "coverage:prod": "coverage-tests create-tests && yarn test:coverage && coverage-tests process-report --send-report prod",
    "test:coverage": "APPLITOOLS_BATCH_NAME='JS Coverage Tests: eyes-testcafe' APPLITOOLS_BATCH_ID=$(uuidgen) XUNIT_FILE=coverage-test-report.xml mocha --config .mocharc.cvg.js"
  },
  "keywords": [
    "eyes-testcafe",
    "eyes-rendering",
    "eyes-visual-grid",
    "applitools",
    "eyes",
    "test automation",
    "visual regression",
    "automation",
    "testing",
    "tests",
    "testcafe"
  ],
  "author": {
    "name": "Applitools Team",
    "email": "team@applitools.com"
  },
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "@applitools/eyes-sdk-core": "12.1.2",
    "@applitools/visual-grid-client": "14.7.3"
  },
  "peerDependencies": {
    "testcafe": "^1.9.0"
  },
  "devDependencies": {
    "@applitools/sdk-coverage-tests": "^1.0.7",
    "@applitools/sdk-fake-eyes-server": "^2.0.0",
    "@applitools/sdk-release-kit": "0.2.2",
    "@applitools/sdk-shared": "0.1.0",
    "eslint": "^7.2.0",
    "mocha": "^8.0.1",
    "spec-xunit-file": "0.0.1-3",
    "testcafe": "^1.9.0"
  },
  "engines": {
    "node": ">=8.9.0"
  }
}
