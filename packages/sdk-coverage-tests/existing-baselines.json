{
  "tests": {
    "defaults": {
      "browserName": "Chrome",
      "platformName": "Linux",
      "testNameSuffix": {
        "isCssStitching": {
          "testNameSuffix": ""
        },
        "isScrollStitching": {
          "testNameSuffix": "_Scroll"
        },
        "isVisualGrid": {
          "testNameSuffix": "_VG"
        }
      },
      "viewportSize": "700x460",
      "URL": "https://applitools.github.io/demo/TestPages/FramesTestPage/"
    },
    "Check Element": [
      {
        "baselineTestName": "TestCheckElementFully_Fluent",
        "appName": "Eyes Selenium SDK - Fluent API",
        "target": "[name='frame1']",
        "commandApi": "fluent"
      },
      {
        "baselineTestName": "TestCheckRegion",
        "appName": "Eyes Selenium SDK - Classic API",
        "target": "#overflowing-div",
        "commandApi": "classic"
      },
      {
        "baselineTestName": "TestCheckRegion2",
        "appName": "Eyes Selenium SDK - Classic API",
        "target": "#overflowing-div-image",
        "commandApi": "classic"
      }
    ],
    "Check Element In Frame": [
      {
        "baselineTestName": "TestCheckRegionInFrame",
        "appName": "Eyes Selenium SDK - Classic API",
        "target": "#inner-frame-div",
        "frameTarget": "[name='frame1']",
        "commandApi": "classic"
      },
      {
        "baselineTestName": "TestCheckRegionInFrame_Fluent",
        "appName": "Eyes Selenium SDK - Fluent API",
        "target": "#inner-frame-div",
        "frameTarget": "[name='frame1']",
        "commandApi": "fluent"
      }
    ],
    "Check Window Viewport": [
      {
        "baselineTestName": "TestCheckWindowViewport",
        "appName": "Eyes Selenium SDK - Classic API",
        "commandApi": "classic"
      },
      {
        "baselineTestName": "TestCheckWindow_Fluent",
        "appName": "Eyes Selenium SDK - Fluent API",
        "commandApi": "fluent"
      }
    ],
    "Check Window Fully": {
      "baselineTestName": "TestCheckWindowFully",
      "appName": "Eyes Selenium SDK - Classic API",
      "commandApi": "classic"
    }
  }
}
