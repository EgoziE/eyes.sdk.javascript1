{
  "name": "eyes.leanft",
  "version": "1.3.1",
  "description": "Applitools Eyes SDK For LeanFT JavaScript",
  "keywords": [
    "eyes.leanft",
    "applitools",
    "eyes",
    "test automation",
    "visual regression",
    "automation",
    "tests",
    "testing",
    "leanft"
  ],
  "homepage": "https://applitools.com",
  "author": {
    "name": "Applitools Team",
    "email": "team@applitools.com",
    "url": "https://applitools.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/applitools/eyes.sdk.javascript1.git"
  },
  "bugs": {
    "url": "https://github.com/applitools/eyes.leanft.javascript/issues"
  },
  "files": [
    "README.md",
    "LICENSE",
    "bundle.js"
  ],
  "main": "./bundle.js",
  "dependencies": {
    "eyes.utils": "portal:../../src/common-legacy",
    "eyes.sdk": "portal:../../src/core-legacy"
  },
  "devDependencies": {
    "@applitools/sdk-release-kit": "portal:../../src/release-kit",
    "mocha": "^5.2.0",
    "uglify-js": "^3.4.0",
    "webpack": "^3.12.0",
    "webpack-node-externals": "^1.7.2"
  },
  "optionalDependencies": {
    "leanft": "*",
    "leanft.report": "*",
    "leanft.report.builder": "*",
    "leanft.sdk.apitesting.uft": "*",
    "leanft.sdk.core": "*",
    "leanft.sdk.insight": "*",
    "leanft.sdk.java": "*",
    "leanft.sdk.mobile": "*",
    "leanft.sdk.sapgui": "*",
    "leanft.sdk.sapui5": "*",
    "leanft.sdk.stdwin": "*",
    "leanft.sdk.te": "*",
    "leanft.sdk.web": "*",
    "leanft.sdk.winforms": "*",
    "leanft.sdk.wpf": "*"
  },
  "scripts": {
    "vv": "bongo --verify-versions",
    "vc": "bongo --verify-changelog",
    "uc": "bongo --update-changelog",
    "test": "mocha ./test/**/*.js",
    "build": "webpack",
    "minify": "uglifyjs -c -m reserved=[module.exports] --warn bundle.js -o bundle.js",
    "prepublishOnly": "npm run build && npm run minify",
    "postpublish": "git push; git push --tags",
    "preversion": "yarn vv && yarn vc",
    "version": "yarn uc && git add CHANGELOG.md"
  },
  "license": "SEE LICENSE IN LICENSE",
  "engines": {
    "node": ">=6.9.0"
  }
}
