      "snapshot": {
        "hook": "beforeAll(async () => {batchName = \"hello world\";});beforeEach(async () => {global.eyes = new Eyes(serverUrl, configuration.params.eyesDisabled);eyes.setApiKey(apiKey);eyes.setAgentId(\"eyes.seleniumide.runner\");eyes.setBatch(batchName, batchId);eyes.setHideScrollbars(true);eyes.setLogHandler(new ConsoleLogHandler(true));});afterEach(async () => {if (eyes._isOpen) {await eyes.close();}});"
      }
    }],
